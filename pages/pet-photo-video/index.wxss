.pet-photo {
  width: 100%;
  height: 100%;
  background-color: #fff;
  overflow: hidden;
}
.pet-photo-title {
  height: 40rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(136,136,136,1);
  line-height: 40rpx;
  margin: 44rpx 0 0 36rpx;
}
.pet-photo-part {
  margin: 50rpx 0 0 39rpx;
}
.pet-photo-item {
  float: left;
  width: 299rpx;
  height: 179rpx;
  margin: 10rpx 37rpx 20rpx 0;
}
.pet-photo-item image,video {
  width: 100%;
  height: 100%;
}