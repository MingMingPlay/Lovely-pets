.swiper-tab{
  width: 100%; 
  text-align: center; 
  height: 100rpx;
  line-height: 100rpx;
  background: rgba(250,250,250,1);
  box-shadow: 0 1rpx 0 0 rgba(212,212,212,1);
}
.swiper-tab-list{
  display: inline-block; 
  width: 50%; 
  font-size: 31rpx;
  font-weight: 400;
  color:#000;
}
.on{
  color: #18CE38; 
  border-bottom: 5rpx solid #18CE38;
}
.tab-label {
  width: 156rpx;
  height: 95rpx;
  line-height: 100rpx;
  margin: 0 auto;
}