.add-tag {
  position: fixed;
  z-index: 99;
  width: 120rpx;
  height: 120rpx;
  background: rgba(59,184,58,1);
  box-shadow: 0 6rpx 12rpx 0 rgba(0,0,0,0.4);
  right: 38rpx;
  bottom: 104rpx;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.add-tag-icon {
  width: 70rpx;
  height: 70rpx;
}
.add-tag-icon image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}